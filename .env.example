# Environment Variables for Supply Chain Tracking System

# Private key of the account that will deploy the contract
# WARNING: Never commit your actual private key to version control
# Use a testnet account with test tokens only
PRIVATE_KEY=your_private_key_here

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Network Configuration
CORE_TESTNET_RPC_URL=https://rpc.test2.btcs.network
CORE_TESTNET_CHAIN_ID=1115

# Development Settings
NODE_ENV=development
DEBUG=true

# Contract Deployment Settings
GAS_LIMIT=6000000
GAS_PRICE=20000000000
